{
    "type": "object",
    "additionalProperties": false,
    "required": ["contractType", "valid", "date"],
    "properties": {
        "contractType"        : {
            "enum"            : [
                "discoverable", "delete", "contactable"
            ],
            "description": "has person agreed to be found by a search (discoverable) or contacted (contactable)"
        },
        "date": {
            "type": "string",
            "format": "date-time",
            "description": "time of creation of legal object"
        },
        "contractDescription" : { "type" : "string"},
        "valid"               : { "type" : "boolean"}
    }
}
