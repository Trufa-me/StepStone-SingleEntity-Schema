{
    "type": "object",
    "additionalProperties": false,
    "required": ["title"],
    "description": "record of a project worked on",
    "properties": {
        "end": {
            "type": "string",
            "format": "date-time",
            "description": "end date of the project"
        },
        "description": {
            "type": "string",
            "description": "description of the project"
        },
        "members": {
            "type": "array",
            "minItems": 1,
            "items": { "$ref": "member.json#" }
        },
        "start": {
            "type": "string",
            "format": "date-time",
            "description": "start date of the project"
        },
        "title": {
            "type": "string",
            "description": "name or title of the project"
        },
        "url": {
            "type": "string",
            "description": "URL address for the project"
        }
    }
}
